@startuml

entity "Игра" as game {
  + номер_игры
  Команда_1
  команда_2
}

entity "Команда" as team {
  + Название
  Кол-во_игр
  Кол-во побед
}

entity "Футболист" as football_player {
  + Имя
  Название_команды
}

entity "Арбитр" as referee {
  + Имя
  судит_номер_игры
}

team --|> game : играет в 
football_player --|> team : Играет в команде
referee --|> game : Судит

@enduml
